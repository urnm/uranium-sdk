{
  "name": "@uranium/react",
  "version": "0.2.2",
  "type": "module",
  "description": "React hooks and components for Uranium SDK",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "bun run clean && bun run build:types && bun run build:js",
    "build:types": "bunx tsc --emitDeclarationOnly",
    "build:js": "bun run build.ts",
    "dev": "bun --watch ./src/index.ts",
    "test": "bun test",
    "lint": "biome check . --diagnostic-level=error",
    "lint:fix": "biome check . --write --diagnostic-level=error",
    "format": "biome format --write .",
    "clean": "rm -rf dist tsconfig.tsbuildinfo",
    "prepublishOnly": "bun run lint && bun run build && bun test"
  },
  "dependencies": {
    "@uranium/sdk": "workspace:*",
    "@uranium/types": "workspace:*",
    "@tanstack/react-query": "^5.0.0",
    "axios": "^1.6.0"
  },
  "peerDependencies": {
    "react": "^18.0.0 || >=19.0.3 <19.1.0 || >=19.1.4 <19.2.0 || >=19.2.3 <20.0.0",
    "react-dom": "^18.0.0 || >=19.0.3 <19.1.0 || >=19.1.4 <19.2.0 || >=19.2.3 <20.0.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@uranium/configs": "workspace:*",
    "typescript": "^5.3.0"
  },
  "keywords": [
    "uranium",
    "react",
    "hooks",
    "sdk",
    "nft"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/urnm/uranium-sdk.git",
    "directory": "packages/react"
  },
  "bugs": {
    "url": "https://github.com/urnm/uranium-sdk/issues"
  },
  "homepage": "https://github.com/urnm/uranium-sdk#readme",
  "author": "Uranium",
  "license": "Apache-2.0"
}
